#!/bin/bash

BASEDIR=/media/matt/phone

cp -a ~/.keepassx/* $BASEDIR/keepassx/.
cp -a ~/.ssh/id_dsa $BASEDIR/ssh/.
echo pulling backups
rsync -aLh --inplace --delete $BASEDIR/backups/ $HOME/workspace/backups/phone
echo done
echo pulling pics
rsync -aLh --inplace $BASEDIR/DCIM/Camera/ $HOME/workspace/pics/photos/new
echo moving pics to gallery
mv $BASEDIR/DCIM/Camera/* $BASEDIR/pics/.
echo done

